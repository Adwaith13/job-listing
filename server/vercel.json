{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      },
      {
        "src": "./routes/auth.js",
        "use": "@vercel/node"
      },
      {
        "src": "./routes/jobpost.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "./index.js",
        "methods": ["GET", "POST", "PATCH","OPTIONS"],
        "headers": {
          "Access-Control-Allow-Origin": "*",
          "Access-Control-Allow-Methods": "GET,PATCH,POST,OPTIONS",
          "Access-Control-Allow-Headers": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version",
          "Access-Control-Allow-Credentials": "true"
        }
      },
      {
        "src": "/auth/register",
        "dest": "./routes/auth.js",
        "methods": ["GET", "POST", "PATCH", "OPTIONS"],
        "headers": {
          "Access-Control-Allow-Origin": "*",
          "Access-Control-Allow-Methods": "GET,PATCH,POST,OPTIONS",
          "Access-Control-Allow-Headers": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version",
          "Access-Control-Allow-Credentials": "true"
        }
      },
      {
        "src": "/auth/login",
        "dest": "./routes/auth.js",
        "methods": ["GET", "POST", "PATCH", "OPTIONS"],
        "headers": {
          "Access-Control-Allow-Origin": "*",
          "Access-Control-Allow-Methods": "GET,PATCH,POST,OPTIONS",
          "Access-Control-Allow-Headers": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version",
          "Access-Control-Allow-Credentials": "true"
        }
      },
      {
        "src": "/job/dashboard",
        "dest": "./routes/jobpost.js",
        "methods": ["GET", "POST", "PATCH", "OPTIONS"],
        "headers": {
          "Access-Control-Allow-Origin": "*",
          "Access-Control-Allow-Methods": "GET,PATCH,POST,OPTIONS",
          "Access-Control-Allow-Headers": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version",
          "Access-Control-Allow-Credentials": "true"
        }
      },
      {
        "src": "/job/jobpost",
        "dest": "./routes/jobpost.js",
        "methods": ["GET", "POST", "PATCH", "OPTIONS"],
        "headers": {
          "Access-Control-Allow-Origin": "*",
          "Access-Control-Allow-Methods": "GET,PATCH,POST,OPTIONS",
          "Access-Control-Allow-Headers": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version",
          "Access-Control-Allow-Credentials": "true"
        }
      },
      {
        "src": "/job/jobpost/:id",
        "dest": "./routes/jobpost.js",
        "methods": ["GET", "POST", "PATCH", "OPTIONS"],
        "headers": {
          "Access-Control-Allow-Origin": "*",
          "Access-Control-Allow-Methods": "GET,PATCH,POST,OPTIONS",
          "Access-Control-Allow-Headers": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version",
          "Access-Control-Allow-Credentials": "true"
        }
      },
      {
        "src": "/job/jobpost/:id",
        "dest": "./routes/jobpost.js",
        "methods": ["GET", "POST", "PATCH", "OPTIONS"],
        "headers": {
          "Access-Control-Allow-Origin": "*",
          "Access-Control-Allow-Methods": "GET,PATCH,POST,OPTIONS",
          "Access-Control-Allow-Headers": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version",
          "Access-Control-Allow-Credentials": "true"
        }
      },
      {
        "src": "/job/jobdata",
        "dest": "./routes/jobpost.js",
        "methods": ["GET", "POST", "PATCH", "OPTIONS"],
        "headers": {
          "Access-Control-Allow-Origin": "*",
          "Access-Control-Allow-Methods": "GET,PATCH,POST,OPTIONS",
          "Access-Control-Allow-Headers": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version",
          "Access-Control-Allow-Credentials": "true"
        }
      },
      {
        "src": "/job/jobdata/:id",
        "dest": "./routes/jobpost.js",
        "methods": ["GET", "POST", "PATCH", "OPTIONS"],
        "headers": {
          "Access-Control-Allow-Origin": "*",
          "Access-Control-Allow-Methods": "GET,PATCH,POST,OPTIONS",
          "Access-Control-Allow-Headers": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version",
          "Access-Control-Allow-Credentials": "true"
        }
      },
      {
        "src": "/job/jobdata/skills",
        "dest": "./routes/jobpost.js",
        "methods": ["GET", "POST", "PATCH", "OPTIONS"],
        "headers": {
          "Access-Control-Allow-Origin": "*",
          "Access-Control-Allow-Methods": "GET,PATCH,POST,OPTIONS",
          "Access-Control-Allow-Headers": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version",
          "Access-Control-Allow-Credentials": "true"
        }
      },
      {
        "src": "/job/jobdata/jobposition",
        "dest": "./routes/jobpost.js",
        "methods": ["GET", "POST", "PATCH", "OPTIONS"],
        "headers": {
          "Access-Control-Allow-Origin": "*",
          "Access-Control-Allow-Methods": "GET,PATCH,POST,OPTIONS",
          "Access-Control-Allow-Headers": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version",
          "Access-Control-Allow-Credentials": "true"
        }
      }
    ]
  }
  